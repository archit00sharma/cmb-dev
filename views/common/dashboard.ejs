                      <div class="card h-100 border-0 verified-card info-card">
                        <div class="card-body">
                          <div class="d-flex align-items-center">
                            <div class="flex-grow-1">
                              <div class="small font-weight-bold text-capitalize text-info mb-1">
                                Team Performance
                              </div>
                              <div class="h5"></div>
                              <div class="text-xs font-weight-bold text-info d-inline-flex align-items-center">
                                <a href="/case/teamEfficiency" class="btn btn-info text-uppercase btn-xs px-2">OPEN
                                  &nbsp; &nbsp; <i class="fad fa-chevron-right"></i></a>
                              </div>
                            </div>
                            <div class="ml-2 fs-50 position-relative">
                              <i class="fad fa-university"></i>
                              <i
                                class="fas fa-check-circle position-absolute fs-22 top-8px right-n8px rounded-pill bg-white text-info"></i>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>


                    <div class="col-xl-4 col-md-4 mb-4">
                      <!-- Dashboard info widget 3-->
                      <div class="card h-100 border-0 verified-card info-card">
                        <div class="card-body">
                          <div class="d-flex align-items-center">
                            <div class="flex-grow-1">
                              <div class="small font-weight-bold text-capitalize text-info mb-1">
                                google map
                              </div>
                              <div class="h5"></div>
                              <div class="text-xs font-weight-bold text-info d-inline-flex align-items-center">
                                <a href="/case/calculateDistance" class="btn btn-info text-uppercase btn-xs px-2">OPEN
                                  &nbsp; &nbsp; <i class="fad fa-chevron-right"></i></a>
                              </div>
                            </div>
                            <div class="ml-2 fs-50 position-relative">
                              <i class="fad fa-university"></i>
                              <i
                                class="fas fa-check-circle position-absolute fs-22 top-8px right-n8px rounded-pill bg-white text-info"></i>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <div class="col-xl-4 col-md-4 mb-4">
                      <!-- Dashboard info widget 3-->
                      <div class="card h-100 border-0 verified-card info-card">
                        <div class="card-body">
                          <div class="d-flex align-items-center">
                            <div class="flex-grow-1">
                              <div class="small font-weight-bold text-capitalize text-info mb-1">
                                TAT Excel
                              </div>
                              <div class="h5"></div>
                              <div class="text-xs font-weight-bold text-info d-inline-flex align-items-center">
                                <a href="/case/calculateTatExcel" class="btn btn-info text-uppercase btn-xs px-2">OPEN
                                  &nbsp; &nbsp; <i class="fad fa-chevron-right"></i></a>
                              </div>
                            </div>
                            <div class="ml-2 fs-50 position-relative">
                              <i class="fad fa-university"></i>
                              <i
                                class="fas fa-check-circle position-absolute fs-22 top-8px right-n8px rounded-pill bg-white text-info"></i>
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>


                    <div class="col-xl-4 col-md-4 mb-4">
                      <!-- Dashboard info widget 3-->
                      <div class="card h-100 border-0 verified-card info-card">
                        <div class="card-body">
                          <div class="d-flex align-items-center">
                            <div class="flex-grow-1">
                              <div class="small font-weight-bold text-capitalize text-info mb-1">
                                FieldExecutive Case History
                              </div>
                              <div class="h5"></div>
                              <div class="text-xs font-weight-bold text-info d-inline-flex align-items-center">
                                <a href="/admin/feCaseHistory" class="btn btn-info text-uppercase btn-xs px-2">OPEN
